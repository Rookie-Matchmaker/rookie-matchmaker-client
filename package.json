{
  "name": "with-chakra-ui-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "lint": "eslint src/** test/** --ext .ts,.tsx --fix",
    "commit": "git add -A && npx cz -S",
    "test": "jest",
    "format": "prettier -w './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc --ignore-path ./.prettierignore",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "postinstall": "husky install && patch-package",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.0",
    "@chakra-ui/react": "^1.0.0",
    "@chakra-ui/theme-tools": "1.0.0",
    "@emotion/react": "11.1.1",
    "@emotion/styled": "11.0.0",
    "framer-motion": "^2.9.4",
    "next": "latest",
    "next-auth": "^3.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "socket.io-client": "^3.1.1",
    "swr": "^0.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.20",
    "@types/next-auth": "^3.1.24",
    "@types/node": "^14.14.25",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^5.0.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "node-jose-tools": "^1.6.26",
    "patch-package": "^6.4.5",
    "prettier": "^2.2.1",
    "socket.io": "^4.0.0",
    "socket.io-mock": "^1.3.1",
    "ts-node": "^9.1.1",
    "typescript": "4.0.5"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
